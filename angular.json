{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "electron_angular_prj": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src/front_src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "allowedCommonJsDependencies": [
              "@aws-crypto/crc32",
              "@aws-crypto/sha256-browser",
              "@aws-crypto/sha256-js",
              "buffer",
              "camelcase-keys",
              "crypto-js/enc-base64",
              "crypto-js/hmac-sha256",
              "crypto-js/core",
              "crypto-js/lib-typedarrays",
              "crypto-js/sha256",
              "fast-xml-parser",
              "isomorphic-unfetch",
              "lodash/cloneDeep",
              "lodash/get",
              "lodash/isEmpty",
              "lodash/isEqual",
              "lodash/includes",
              "qrcode",
              "style-dictionary/lib/utils/deepExtend",
              "style-dictionary/lib/utils/flattenProperties",
              "ulid",
              "url",
              "uuid",
              "xstate/lib/actions"
            ],
            "outputPath": "dist",
            "index": "src/front_src/index.html",
            "main": "src/front_src/main.ts",
            "budgets": [
              {
                "type": "initial",
                "maximumWarning": "1mb",
                "maximumError": "3mb"
              },
              {
                "type": "anyComponentStyle",
                "maximumWarning": "20kb",
                "maximumError": "40kb"
              }
            ],
            "polyfills": "src/front_src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["src/front_src/favicon.ico", "src/front_src/assets"],
            "styles": ["src/front_src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/front_src/environments/environment.ts",
                  "with": "src/front_src/environments/environment.amplify_prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "amplify-dev": {
              "fileReplacements": [
                {
                  "replace": "src/front_src/environments/environment.ts",
                  "with": "src/front_src/environments/environment.amplify_dev.ts"
                }
              ],
              "outputHashing": "all"
            },
            "amplify-staging": {
              "fileReplacements": [
                {
                  "replace": "src/front_src/environments/environment.ts",
                  "with": "src/front_src/environments/environment.amplify_staging.ts"
                }
              ],
              "outputHashing": "all"
            },
            "amplify-prod": {
              "fileReplacements": [
                {
                  "replace": "src/front_src/environments/environment.ts",
                  "with": "src/front_src/environments/environment.amplify_prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "electron_angular_prj:build:production"
            },
            "development": {
              "browserTarget": "electron_angular_prj:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "electron_angular_prj:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/front_src/test.ts",
            "polyfills": "src/front_src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": ["src/front_src/favicon.ico", "src/front_src/assets"],
            "styles": ["src/front_src/styles.scss"],
            "scripts": []
          }
        }
      }
    }
  }
}
